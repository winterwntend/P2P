#include <iostream>
#include <string>
using namespace std;
int main () {
    int number;
    string district;
    cout << "Enter number of circle moscow(1-3):";
    cin >> number;
    if (number == 1 ){
        district = "CAO";
    } else if (number == 2){
        district = "YOUVAO";
    } else if (number == 3 ) {
        district = "BAO";
    } else {
        district = "not stated circle";
    }
    cout << "Name of circle:" <<
    district << endl;
    return 0;
}
