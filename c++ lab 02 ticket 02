#include <iostream>

using namespace std;

int main() {
  int a, b, c;

  cout << "Введите длины отрезков (через пробел): ";
  cin >> a >> b >> c;

  // Проверяем, выполняется ли неравенство треугольника
  if (a + b > c && a + c > b && b + c > a) {
    cout << "Из этих отрезков можно построить треугольник." << endl;
  } else {
    cout << "Из этих отрезков нельзя построить треугольник." << endl;
  }

  return 0;
}
